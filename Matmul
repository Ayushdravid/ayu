#include<stdio.h>
void matmul(int a[][10],int b[][10],int c[][10],int,int,int,int);
int main()
{
    int m,n,p,q,i,j,k,a[10][10],b[10][10],c[10][10];
    printf("enter the size of matrix A\n");
    scanf("%d%d",&m,&n);
    printf("enter the size of matrixB\n");
    scanf("%d%d",&p,&q);
    if(n!=p)
      {  
        printf("multiplication not possible\n");
      }
    else
    { 
       printf("enter matrix A\n");
       for(i=0;i<m;i++)
         {
            for(j=0;j<n;j++)
            {
                scanf("%d",&a[i][j]);
            }
         }
       printf("enter matrix B\n");
         for(i=0;i<m;i++) 
           {
               for(j=0;j<n;j++)
               {
                   scanf("%d",&b[i][j]);
               }
           }
       matmul(a,b,c,m,n,p,q); 
       printf("resultant matrix \n");
       for(i=0;i<m;i++)
         {
             for(j=0;j<q;j++)
             {
                printf("%d",c[i][j]);
             }
         }
    }
    matmul(int a[][10],int b[][10],int c[][10],int m,int n,int p,int q)
    {
        int i,j,k;
        for(i=0;i<m;i++)
         {
            for(j=0;j<q;j++)
            {
                c[i][j]=0;
            }
         } 
         for(i=0;i<m;i++)
           {
             for(j=0;j<q;j++)
               {
                   for(k=0;k<n;k++)
                    {
                      c[i][j]=c[i][j]+a[i][k]*b[k][j];
                    }
               }
           }
    }
         
